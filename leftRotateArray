class GFG {
	public static void main (String[] args) {
		//code
		Scanner in=new Scanner(System.in);
		int T=in.nextInt();
	for(int i=0;i<T;i++)
	{
	   int N=in.nextInt();
	   int[]arr=new int[N];
	   int D=in.nextInt();
	   for(i=0;i<N;i++)
	   {
	       arr[i]=in.nextInt();
	   }
	   for(i=0;i<N;i++)
	   {
	       arr[i]= arr[(i+D)%N];
	   }
	   
	   for(i=0;i<N;i++)
	   {
	       System.out.println(arr[i]);
	   }
	   
	}
	
	}
}
