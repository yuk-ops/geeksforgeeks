class Solution {
    int MissingNumber(int array[], int n) {
        // Your Code Here
        int ans=0;
        int sum=0;
        for(int i=1;i<=n;i++)
        {
            sum+=i;
        }
      for(int i=0;i<array.length;i++)
      {
          ans+=array[i];
      }
      return sum-ans;
    }
}
